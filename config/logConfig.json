{
  "appenders": [
    {
      "type": "console"
    },
    {
      "type": "dateFile",
      "filename": "./logs/Expense",
      "pattern": "-yyyy-MM-dd.log",
      "alwaysIncludePattern":true,
      "layout": {
        "type": "pattern",
        "pattern": "[%d{yyyy-MM-dd hh:mm:ss.SSS}] [%-5p] [%-20c] - %m"
      }
    }
  ]
}